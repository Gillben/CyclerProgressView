<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomCircleProgressView">
        <attr name="custom_paint_with" format="dimension" />
        <attr name="custom_paint_color" format="color" />
        <attr name="custom_text_size" format="dimension" />
        <attr name="custom_text_color" format="color" />

        <attr name="sweep_location" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>

    </declare-styleable>

</resources>